import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

public static void main(String[] args) {
        Scanner nn = new Scanner(System.in);
        
        String a = nn.nextLine();
        String b = "";

        for (int i = 0; i < a.length(); i++) {
            char ch = a.charAt(i);
            
            if (ch != ' ') {
                b+= ch;
            }
        }

        System.out.println(b);
    }
}
