import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner hi = new  Scanner(System .in);
         String name = hi.nextLine();
        int arrear = hi.nextInt();
        int cgpa = hi.nextInt();

        if (arrear == 1 && cgpa > 60)
        {
            System.out.println( "Name of the Student:" + name);
            System.out.println(name + " is Eligible for Placement");
        } else if ((arrear == 1 || arrear == 2) && cgpa > 75)
        {
            System.out.println( "Name of the Student:" + name);
            System.out.println(name + " is Eligible for Placement");
        } else
        {
            System.out.println( "Name of the Student: " + name);0132
            System.out.println(name + " is Not Eligible for Placement");
        }   
      
    }
}
